---to get inside a database
use noob_db;
---create a table in db
create table if not exist employee
( id INT,
emp_name VARCHAR(20) );
--- to show the table
show table;
--- command to see the table defination
show create table employee;
---create employee table with few more table
create table if not exist employee(
id int,
name varvhar(20),
salary double,
hiring_date date
);
---syntax to insert data into a table
insert into employee values(01,'shaku',5000,2015-05-21);
---syntax 2 insert data into table
insert into employee(salary,name,id) values(1000,"raju",2);
---command to insert multiple record into a table
insert into employee values(3,"shyam",1555,2015-05-06),
(4,"shyamalal",15000,2015-04-04),(5,"raja",2005,2025-07-06);
---how to query or fetch data from a table
select * from employee;

---copy item---command to see the list of database
show database;
